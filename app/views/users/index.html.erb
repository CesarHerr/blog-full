<section>
  <ul class="col-md-12">
    <% @users&.each do |user| %>
      <li class="col-md-4">

        <%= link_to user_path(user) do %>
          <h5 class="card-title"><%= user.username %></h5>
        <% end %>  
          <p><%= user.bio %></p>
        
      </li>
    <% end %>
  </ul>
</section>
